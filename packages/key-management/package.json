{
  "name": "@starkms/key-management",
  "description": "An agnostic key management package for starkms",
  "version": "0.0.1",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bunx --bun tsc --noEmit && bunx --bun tsup",
    "dev": "bunx --bun tsup --watch",
    "test:unit": "bun test",
    "coverage": "bun test --coverage",
    "cleanup": "bunx --bun rimraf node_modules dist .turbo"
  },
  "dependencies": {
    "@noble/ciphers": "0.5.3",
    "@noble/hashes": "1.4.0",
    "@noble/secp256k1": "2.1.0",
    "@starkms/crypto": "workspace:*",
    "@starkms/starkms-core": "workspace:*",
    "@scure/base": "1.1.7",
    "@scure/bip32": "1.4.0",
    "@scure/bip39": "1.3.0",
    "@scure/starknet": "^1.1.0",
    "buffer": "6.0.3",
    "ethers": "^6.13.4",
    "micro-starknet": "^0.2.3",
    "starknet": "^6.11.0",
    "ts-custom-error": "3.3.1"
  },
  "devDependencies": {
    "@starkms/common": "workspace:*"
  }
}
